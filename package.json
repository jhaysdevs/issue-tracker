{
  "name": "issue-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,ts,tsx,jsx,json,css,scss,md}\"",
    "db:up": "cd docker && docker-compose up -d",
    "db:down": "cd docker && docker-compose down",
    "db:logs": "cd docker && docker-compose logs -f",
    "db:reset": "cd docker && docker-compose down -v && docker-compose up -d",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
